s:AddToggle("\065\105\109\098\111\116\084\114\097\099\101\114",{Text="\084\097\114\103\101\116 \084\114\097\099\101\114"})):AddColorPicker("\065\105\109\098\111\116\084\114\097\099\101\114\067\111\108\111\114",{Title="\084\114\097\099\101\114 \067\111\108\111\114",Default=Color3.fromRGB(255,255,255),Transparency=0})Options.AimbotTracerColor:OnChanged(function()e.Tracer.Color=Options.AimbotTracerColor.Value e.Tracer.Transparency=1-Options.AimbotTracerColor.Transparency end)end do Toggles.AimbotTracer:OnChanged(function(s)e.Tracer.Visible=s and Toggles.AimbotToggle.Value or false end)Toggles.AimbotToggle:OnChanged(function(s)e.Tracer.Visible=s and Toggles.AimbotTracer.Value or false end)end end for s,e in pairs({e.Circle})do e.Visible=false e.Thickness=1.5 e.Color=n.AccentColor end for s,e in pairs({e.Tracer})do e.Visible=false e.Thickness=1.5 end C(.05,l)L(p.RenderStepped,b)L(p.Stepped,V)L(p.RenderStepped,W)L(c.Move,a)C(1,function()for s,e in pairs(Y:GetChildren())do local a=e.ClassName=="\080\108\097\121\101\114"and e.Character or e if not a:GetAttribute("\069\118\099\069\083\080")then a:SetAttribute("\069\118\099\069\083\080",true)if e.Name~=x.Name and a then f(e)end end end end)end
