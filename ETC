ggle("\065\105\109\098\111\116\084\111\103\103\108\101",{Text="\083\116\097\116\117\115"})):AddKeyPicker("\065\105\109\098\111\116\075\101\121\098\105\110\100",{Text="\065\105\109\098\111\116";Default="\077\066\050";Mode="\072\111\108\100";Modes={"\084\111\103\103\108\101","\072\111\108\100","\065\108\119\097\121\115"}})R:AddDivider()R:AddToggle("\065\105\109\098\111\116\083\116\105\099\107\121",{Text="\083\116\105\099\107\121 \097\105\109",Tooltip="\083\116\105\099\107\115 \116\111 \116\104\101 \116\097\114\103\101\116"})R:AddToggle("\065\105\109\098\111\116\080\114\101\100\105\099\116\105\111\110",{Text="\080\114\101\100\105\099\116\105\111\110"})local T=R:AddDependencyBox()T:AddSlider("\065\105\109\098\111\116\080\114\101\100\105\099\116\105\111\110\072\111\114\105\122\111\110\116\097\108",{Text="\072\111\114\105\122\111\110\116\097\108",Compact=true,Max=.3;Min=.01,Default=.132,Rounding=3})T:AddSlider("\065\105\109\098\111\116\080\114\101\100\105\099\116\105\111\110\086\101\114\116\105\099\097\108",{Text="\086\101\114\116\105\099\097\108";Compact=true;Max=.3;Min=.01,Default=.132,Rounding=3})T:SetupDependencies({{Toggles.AimbotPrediction;true}})R:AddToggle("\065\105\109\098\111\116\083\109\111\111\116\104\110\101\115\115",{Text="\083\109\111\111\116\104\110\101\115\115"})local j=R:AddDependencyBox()j:AddSlider("\065\105\109\098\111\116\083\109\111\111\116\104\110\101\115\115\072\111\114\105\122\111\110\116\097\108",{Text="\072\111\114\105\122\111\110\116\097\108",Compact=true,Default=30,Max=100,Min=1;Rounding=0,Suffix="\037"})j:AddSlider("\065\105\109\098\111\116\083\109\111\111\116\104\110\101\115\115\086\101\114\116\105\099\097\108",{Text="\086\101\114\116\105\099\097\108",Compact=true,Default=30;Max=100,Min=1,Rounding=0,Suffix="\037"})j:SetupDependencies({{Toggles.AimbotSmoothness;true}})R:AddDropdown("\065\105\109\098\111\116\065\105\109\098\111\110\101",{Text="\065\105\109-\098\111\110\101",Values={"\072\101\097\100";"\072\117\109\097\110\111\105\100\082\111\