,Q.Y)n.PointB=l(G.X,G.Y)n.PointC=l(S.X,S.Y)n.PointD=l(m.X,m.Y)n.Color=Options.ESPBoxesColor.Value n.Transparency=1-Options.ESPBoxesColor.Transparency else h.Box.Visible=false end end);(Q:GetPropertyChangedSignal("\080\097\114\101\110\116")):Connect(function()if not Q.Parent then m:Disconnect()for n,Q in pairs(h)do Q:Remove()end return end end)end local Z=function()local n=Q.Tracer if not Toggles.AimbotTracer.Value then return end if not n.Visible then return end if not Q.Position or not Q.Target then n.To=l(0,10000)n.From=l(0,10000)return end local h,G=T:WorldToViewportPoint(Q.Position)if not G then n.To=l(0,10000)n.From=l(0,10000)end n.From=l(g.X,g.Y+a.Y)n.To=l(h.X,h.Y)end v:OnUnload(function()v.Unloaded=true;(getgenv()).Evc=false cleardrawcache()for n,Q in pairs(n)do Q:Disconnect()end for n,Q in pairs(s:GetChildren())do local h=Q.ClassName=="\080\108\097\121\101\114"and Q.Character or Q if h:GetAttribute("\069\118\099\069\083\080")then h:SetAttribute("\069\118\099\069\083\080",false)end end end)do do local n=(Y.Main:AddLeftTabbox()):AddTab("\065\105\109\105\110\103");(n:AddToggle("\065\105\109\098\111\116\084\111\103\103\108\101",{Text="\083\116\097\116\117\115"})):AddKeyPicker("\065\105\109\098\111\116\075\101\121\098\105\110\100",{Text="\065\105\109\098\111\116";Default="\077\066\050",Mode="\072\111\108\100";Modes={"\084\111\103\103\108\101","\072\111\108\100";"\065\108\119\097\121\115"}})n:AddDivider()n:AddToggle("\065\105\109\098\111\116\083\116\105\099\107\121",{Text="\083\116\105\099\107\121 \097\105\109",Tooltip="\083\116\105\099\107\115 \116\111 \116\104\101 \116\097\114\103\101\116"})n:AddToggle("\065\105\109\098\111\116\080\114\101\100\105\099\116\105\111\110",{Text="\080\114\101\100\105\099\116\105\111\110"})local Q=n:AddDependencyBox()Q:AddSlider("\065\105\109\098\111\116\080\114\101\100\105\099\116\105\111\110\072\111\114\105\122\111\110\116\097\108",{Text="\072\111\114\105\122\111\110\116\097\108",Compact=true;Max=.3,Min=.01,Default=.132,Rounding=3})Q:AddSlider("\065\105\109\098\111\116\080\114\101\100\105\099\116\105\111\110\086\101\114\116\105\099\097\108",{Text="\086\101\114\116\105\099\097\108",Compact=true,Max=.3,Min=.01;Default=.132;Rounding=3})Q:SetupDependencies({{Toggles.AimbotPrediction,true}})n:AddToggle("\065\105\109\098\111\116\083\109\111\111\116\104\110\101\115\115",{Text="\083\109\111\111\116\104\110\101\115\115"})local h=n:AddDependencyBox()h:AddSlider("\065\105\109\098\111\116\083\109\111\111\116\104\110\101\115\115\072\111\114\105\122\111\110\116\097\108",{Text="\072\111\114\105\122\111\110\116\097\108";Compact=true,Default=30;Max=100,Min=1,Rounding=0;Suffix="\037"})h:AddSlider("\065\105\109\098\111\116\083\109\111\111\116\104\110\101\115\115\086\101\114\116\105\099\097\108",{Text="\086\101\114\116\105\099\097\108",Compact=true,Default=30;Max=100;Min=1,Rounding=0;Suffix="\037"})h:SetupDependencies({{Toggles.AimbotSmoothness;true}})n:AddDropdown("\065\105\109\098\111\116\065\105\109\098\111\110\101",{Text="\065\105\109-\098\111\110\101",Values={"\072\101\097\100","\072\117\109\097\110\111\105\100\082\111\111\116\080\097\114\116"},Default=1})n:AddDivider()n:AddToggle("\065\105\109\098\111\116\085\115\101\070\079\086",{Text="\085\115\101 \070\079\086"})local G=n:AddDependencyBox();(G:AddToggle("\065\105\109\098\111\116\068\114\097\119\070\079\086",{Text="\068\114\097\119"})):AddColorPicker("\065\105\109\098\111\116\067\111\108\111\114\070\079\086",{Title="\065\105\109\098\111\116 \070\079\086 \067\111\108\111\114";Default=v.AccentColor,Transparency=0})G:AddSlider("\065\105\109\098\111\116\083\105\122\101\070\079\086",{Text="\083\105\122\101";Compact=true;Suffix="\112\120",Default=100;Min=0;Max=500,Rounding=0})G:SetupDependencies({{Toggles.AimbotUseFOV,true}})n:AddDivider()n:AddDropdown("\065\105\109\098\111\116\067\104\101\099\107\115",{Text="\067\104\101\099\107\115";AllowNull=true;Multi=true,Values={"\072\101\097\108\116\104","\070\111\114\099\101\070\105\101\108\100","\084\101\097\109";"\087\097\108\108"}})end do do do Toggles.AimbotToggle:OnChanged(function(n)Q.Circle.Visible=n and(Toggles.AimbotDrawFOV.Value and Toggles.AimbotUseFOV.Value)or false end)Toggles.AimbotUseFOV:OnChanged(function(n)Q.Circle.Visible=n and(Toggles.AimbotDrawFOV.Value and Toggles.AimbotToggle.Value)or false end)Toggles.AimbotDrawFOV:OnChanged(function(n)Q.Circle.Visible=n and(Toggles.AimbotUseFOV.Value and Toggles.AimbotToggle.Value)or false end)end Options.AimbotSizeFOV:OnChanged(function(n)Q.Circle.Radius=n end)Options.AimbotColorFOV:OnChanged(function()Q.Circle.Color=Options.AimbotColorFOV.Value Q.Circle.Transparency=1-Options.AimbotColorFOV.Transparency end)end end end do do local n=(Y.Main:AddRightTabbox()):AddTab("\069\083\080")n:AddToggle("\069\083\080\083\116\097\116\117\115",{Text="\083\116\097\116\117\115"})n:AddDivider();((n:AddToggle("\069\083\080\078\097\109\101\116\097\103\115",{Text="\078\097\109\101\116\097\103\115"})):AddColorPicker("\069\083\080\078\097\109\101\116\097\103\115\067\111\108\111\114",{Title="\078\097\109\101\116\097\103\115 \067\111\108\111\114",Default=Color3.fromRGB(255,255,255);Transparency=0})):AddColorPicker("\069\083\080\078\097\109\101\116\097\103\115\079\117\116\108\105\110\101\067\111\108\111\114",{Title="\078\097\109\101\116\097\103\115 \079\117\116\108\105\110\101 \067\111\108\111\114";Default=Color3.fromRGB(10,10,10);Transparency=nil});(n:AddToggle("\069\083\080\066\111\120\101\115",{Text="\066\111\120\101\115"})):AddColorPicker("\069\083\080\066\111\120\101\115\067\111\108\111\114",{Title="\066\111\120\101\115 \067\111\108\111\114";Default=v.AccentColor;Transparency=0})n:AddDivider()n:AddDropdown("\069\083\080\067\104\101\099\107\115",{Text="\067\104\101\099\107\115";Values={"\084\101\097\109"};Multi=true;AllowNull=true})end end do do local n=(Y.Main:AddRightTabbox()):AddTab("\086\105\115\117\097\108\115");(n:AddToggle("\065\105\109\098\111\116\084\114\097\099\101\114",{Text="\084\097\114\103\101\116 \084\114\097\099\101\114"})):AddColorPicker("\065\105\109\098\111\116\084\114\097\099\101\114\067\111\108\111\114",{Title="\084\114\097\099\101\114 \067\111\108\111\114";Default=Color3.fromRGB(255,255,255);Transparency=0})Options.AimbotTracerColor:OnChanged(function()Q.Tracer.Color=Options.AimbotTracerColor.Value Q.Tracer.Transparency=1-Options.AimbotTracerColor.Transparency end)end do Toggles.AimbotTracer:OnChanged(function(n)Q.Tracer.Visible=n and Toggles.AimbotToggle.Value or false end)Toggles.AimbotToggle:OnChanged(function(n)Q.Tracer.Visible=n and Toggles.AimbotTracer.Value or false end)end end for n,Q in pairs({Q.Circle})do Q.Visible=false Q.Thickness=1.5 Q.Color=v.AccentColor end for n,Q in pairs({Q.Tracer})do Q.Visible=false Q.Thickness=1.5 end d(.05,W)o(X.RenderStepped,J)o(X.Stepped,L)o(X.RenderStepped,Z)o(g.Move,h)d(1,function()for n,Q in pairs(s:GetChildren())do local h=Q.ClassName=="\080\108\097\121\101\114"and Q.Character or Q if not h:GetAttribute("\069\118\099\069\083\080")then h:SetAttribute("\069\118\099\069\083\080",true)if Q.Name~=D.Name and h then r(Q)end end end end)end
