)or false end)Toggles.AimbotDrawFOV:OnChanged(function(C)t.Circle.Visible=C and(Toggles.AimbotUseFOV.Value and Toggles.AimbotToggle.Value)or false end)end Options.AimbotSizeFOV:OnChanged(function(C)t.Circle.Radius=C end)Options.AimbotColorFOV:OnChanged(function()t.Circle.Color=Options.AimbotColorFOV.Value t.Circle.Transparency=1-Options.AimbotColorFOV.Transparency end)end end end do do local C=(b.Main:AddRightTabbox()):AddTab("\069\083\080")C:AddToggle("\069\083\080\083\116\097\116\117\115",{Text="\083\116\097\116\117\115"})C:AddDivider();((C:AddToggle("\069\083\080\078\097\109\101\116\097\103\115",{Text="\078\097\109\101\116\097\103\115"})):AddColorPicker("\069\083\080\078\097\109\101\116\097\103\115\067\111\108\111\114",{Title="\078\097\109\101\116\097\103\115 \067\111\108\111\114",Default=Color3.fromRGB(255,255,255),Transparency=0})):AddColorPicker("\069\083\080\078\097\109\101\116\097\103\115\079\117\116\108\105\110\101\067\111\108\111\114",{Title="\078\097\109\101\116\097\103\115 \079\117\116\108\105\110\101 \067\111\108\111\114",Default=Color3.fromRGB(10,10,10);Transparency=nil});(C:AddToggle("\069\083\080\066\111\120\101\115",{Text="\066\111\120\101\115"})):AddColorPicker("\069\083\080\066\111\120\101\115\067\111\108\111\114",{Title="\066\111\120\101\115 \067\111\108\111\114";Default=V.AccentColor;Transparency=0})C:AddDivider()C:AddDropdown("\069\083\080\067\104\101\099\107\115",{Text="\067\104\101\099\107\115",Values={"\084\101\097\109"};Multi=true;AllowNull=true})end end do do local C=(b.Main:AddRightTabbox()):AddTab("\086\105\115\117\097\108\115");(C:AddToggle("\065\105\109\098\111\116\084\114\097\099\101\114",{Text="\084\097\114\103\101\116 \084\114\097\099\101\114"})):AddColorPicker("\065\105\109\098\111\116\084\114\097\099\101\114\067\111\108\111\114",{Title="\084\114\097\099\101\114 \067\111\108\111\114";Default=Color3.fromRGB(255,255,255);Transparency=0})Options.AimbotTracerColor:OnChanged(function()t.Tracer.Color=Options.AimbotTracerColor.Value t.Tra