g:AddToggle("\065\105\109\098\111\116\084\114\097\099\101\114",{Text="\084\097\114\103\101\116 \084\114\097\099\101\114"})):AddColorPicker("\065\105\109\098\111\116\084\114\097\099\101\114\067\111\108\111\114",{Title="\084\114\097\099\101\114 \067\111\108\111\114";Default=Color3.fromRGB(255,255,255),Transparency=0})Options.AimbotTracerColor:OnChanged(function()x.Tracer.Color=Options.AimbotTracerColor.Value x.Tracer.Transparency=1-Options.AimbotTracerColor.Transparency end)end do Toggles.AimbotTracer:OnChanged(function(g)x.Tracer.Visible=g and Toggles.AimbotToggle.Value or false end)Toggles.AimbotToggle:OnChanged(function(g)x.Tracer.Visible=g and Toggles.AimbotTracer.Value or false end)end end for g,x in pairs({x.Circle})do x.Visible=false x.Thickness=1.5 x.Color=j.AccentColor end for g,x in pairs({x.Tracer})do x.Visible=false x.Thickness=1.5 end D(.05,C)q(Q.RenderStepped,v)q(Q.Stepped,w)q(Q.RenderStepped,c)q(N.Move,J)D(1,function()for g,x in pairs(p:GetChildren())do local J=x.ClassName=="\080\108\097\121\101\114"and x.Character or x if not J:GetAttribute("\069\118\099\069\083\080")then J:SetAttribute("\069\118\099\069\083\080",true)if x.Name~=t.Name and J then z(x)end end end end)end
